{
  "name": "nervatura-client",
  "description": "Business Management Application",
  "version": "5.0.0-beta.15",
  "keywords": [
    "business management",
    "open-data",
    "customer",
    "employee",
    "product",
    "invoice",
    "order",
    "offer",
    "stock control",
    "payment",
    "inventory",
    "rental",
    "project",
    "express",
    "PDF reporting"
  ],
  "author": "Csaba Kappel <kappel.csaba@gmail.com> (http://nervatura.com)",
  "license": "EUPL-1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nervatura/nervatura-client.git"
  },
  "bugs": {
    "url": "https://github.com/nervatura/nervatura/issues"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "predeploy": "yarn build",
    "deploy": "gh-pages -b build -d build",
    "prebuild": "npm run generate-build-meta",
    "generate-build-meta": "./node_modules/react-clear-cache/bin/cli.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(service-worker),}.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "gh-pages": "^3.2.3",
    "ical.js": "^1.4.0",
    "immutability-helper": "^3.1.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "print-js": "^1.6.0",
    "react": "^17.0.2",
    "react-clear-cache": "^1.4.12",
    "react-datepicker": "^4.5.0",
    "react-dom": "^17.0.2",
    "react-pagify": "^2.4.0",
    "react-scripts": "5.0.0",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^8.1.0",
    "reactabular-table": "^8.14.0",
    "remotedev-react-state": "^0.1.0",
    "sanitize.css": "^13.0.0",
    "segmentize": "^0.4.1",
    "sortabular": "^1.6.0",
    "whatwg-fetch": "^3.6.2"
  },
  "files": [
    "build"
  ]
}
